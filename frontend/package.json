    "test:bookings": "npx ts-node tests/api/bookings.test.ts",
    "test:stages": "npx ts-node tests/api/stages.test.ts",
    "test:watch": "npm run test:api && nodemon --exec \"npm run test:api\"",
    "test:coverage": "npm run test:api -- --coverage",
    "test:ci": "npm run test:api && npm run test:integration && npm run test:performance"
